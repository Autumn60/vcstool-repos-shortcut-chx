{
  "name": "vcstool repos shortcut extension",
  "version": "0.1.0",
  "manifest_version": 3,
  "description": "Extension to add link buttons to each repository in repos file",
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["main.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "/icons/icon16.png",
    "48": "/icons/icon48.png",
    "128": "/icons/icon128.png"
  }
}
